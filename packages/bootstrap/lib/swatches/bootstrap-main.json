{
    "$schema": "https://unpkg.com/@progress/kendo-theme-tasks@1.14.0/lib/schemas/kendo-swatch.json",
    "name": "Bootstrap Main",
    "product": "kendo",
    "base": "@progress/kendo-theme-bootstrap",
    "version": "5.1.2-dev.7",
    "api": "modern",
    "previewColors": [
        "#ffffff",
        "#f8f9fa",
        "#0d6efd",
        "#0a58ca",
        "#212529"
    ],
    "components": [],
    "groups": [
        {
            "name": "New color system",
            "colorSystem": {
                "kendo-enable-color-system": {
                    "name": "New color system",
                    "type": "boolean",
                    "value": true
                }
            }
        },
        {
            "name": "Theme colors",
            "variables": {
                "kendo-is-dark-theme": {
                    "name": "Dark theme",
                    "type": "boolean",
                    "value": false
                },
                "kendo-color-primary": {
                    "name": "Primary",
                    "type": "color",
                    "value": "#0d6efd"
                },
                "kendo-color-secondary": {
                    "name": "Secondary",
                    "type": "color",
                    "value": "#6c757d"
                },
                "kendo-color-info": {
                    "name": "Info",
                    "type": "color",
                    "value": "#0dcaf0"
                },
                "kendo-color-success": {
                    "name": "Success",
                    "type": "color",
                    "value": "#198754"
                },
                "kendo-color-warning": {
                    "name": "Warning",
                    "type": "color",
                    "value": "#ffc107"
                },
                "kendo-color-error": {
                    "name": "Error",
                    "type": "color",
                    "value": "#dc3545"
                }
            }
        },
