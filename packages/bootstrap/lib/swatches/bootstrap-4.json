                "kendo-series-d": {
                    "name": "Series D",
                    "type": "color",
                    "value": "#f0ad4e"
                },
                "kendo-series-e": {
                    "name": "Series E",
                    "type": "color",
                    "value": "#e67d4a"
                },
                "kendo-series-f": {
                    "name": "Series F",
                    "type": "color",
                    "value": "#d9534f"
