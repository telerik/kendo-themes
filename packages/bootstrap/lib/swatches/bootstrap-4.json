{
    "$schema": "https://unpkg.com/@progress/kendo-theme-tasks@1.14.0/lib/schemas/kendo-swatch.json",
    "name": "Bootstrap 4",
    "product": "kendo",
    "base": "@progress/kendo-theme-bootstrap",
    "version": "5.1.2-dev.7",
    "previewColors": [
        "#ffffff",
        "#f8f9fa",
        "#007bff",
        "#0056b3",
        "#212529"
    ],
    "components": [],
    "groups": [
        {
            "name": "Theme colors",
            "variables": {
                "kendo-is-dark-theme": {
                    "name": "Dark theme",
