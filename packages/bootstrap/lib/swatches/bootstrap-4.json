                "kendo-color-primary": {
                    "name": "Primary",
                    "type": "color",
                    "value": "#007bff"
                },
                "kendo-color-secondary": {
                    "name": "Secondary",
                    "type": "color",
                    "value": "#6c757d"
                },
                "kendo-color-info": {
                    "name": "Info",
                    "type": "color",
                    "value": "#17a2b8"
                },
                "kendo-color-success": {
                    "name": "Success",
                    "type": "color",
                    "value": "#28a745"
                },
                "kendo-color-warning": {
                    "name": "Warning",
                    "type": "color",
                    "value": "#ffc107"
                },
                "kendo-color-error": {
                    "name": "Error",
                    "type": "color",
                    "value": "#dc3545"
                }
            }
        },
        {
            "name": "Body",
            "variables": {
                "kendo-body-text": {
                    "name": "Body text color",
