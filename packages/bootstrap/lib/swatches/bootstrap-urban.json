{
    "$schema": "https://unpkg.com/@progress/kendo-theme-tasks@1.14.0/lib/schemas/kendo-swatch.json",
    "name": "Bootstrap Urban",
    "product": "kendo",
    "base": "@progress/kendo-theme-bootstrap",
    "version": "5.1.2-dev.7",
    "previewColors": [
        "#ffffff",
        "#e3eef4",
        "#ff5c1a",
        "#e65317",
        "#000000"
    ],
    "components": [],
    "groups": [
        {
            "name": "Theme colors",
            "variables": {
                "kendo-is-dark-theme": {
                    "name": "Dark theme",
                    "type": "boolean",
                    "value": false
                },
                "kendo-color-primary": {
                    "name": "Primary",
                    "type": "color",
                    "value": "#ff5c1a"
                },
                "kendo-color-secondary": {
                    "name": "Secondary",
                    "type": "color",
                    "value": "#73a9c9"
                },
                "kendo-color-info": {
                    "name": "Info",
                    "type": "color",
                    "value": "#5bc0de"
                },
                "kendo-color-success": {
                    "name": "Success",
                    "type": "color",
                    "value": "#5cb85c"
                },
                "kendo-color-warning": {
                    "name": "Warning",
                    "type": "color",
                    "value": "#ffbb5b"
                },
                "kendo-color-error": {
                    "name": "Error",
                    "type": "color",
                    "value": "#e3470e"
                }
            }
        },
        {
            "name": "Body",
            "variables": {
                "kendo-body-text": {
                    "name": "Body text color",
                    "type": "color",
                    "value": "#000000"
                },
                "kendo-body-bg": {
                    "name": "Body background",
                    "type": "color",
                    "value": "#ffffff"
                },
                "kendo-subtle-text": {
                    "name": "Subtle text color",
                    "type": "color",
                    "value": "#6c757d"
                },
                "kendo-disabled-text": {
                    "name": "Disabled text color",
                    "type": "color",
                    "value": "#8f8f8f"
                }
            }
        },
        {
            "name": "Components",
            "variables": {
                "kendo-component-color": {
                    "name": "Component text",
                    "type": "color",
                    "value": "#1c3a4c"
                },
                "kendo-component-bg": {
                    "name": "Component background",
                    "type": "color",
                    "value": "#ffffff"
                },
                "kendo-base-text": {
                    "name": "Header text color",
                    "type": "color",
                    "value": "#1c3a4c"
                },
                "kendo-base-bg": {
                    "name": "Header background",
                    "type": "color",
                    "value": "#e3eef4"
                },
                "kendo-hover-text": {
                    "name": "Hover text color",
                    "type": "color",
                    "value": "#16181b"
                },
                "kendo-hover-bg": {
                    "name": "Hover background",
                    "type": "color",
                    "value": "#cfdee6"
                },
                "kendo-selected-text": {
                    "name": "Selected text color",
                    "type": "color",
                    "value": "#ffffff"
                },
                "kendo-selected-bg": {
                    "name": "Selected background",
                    "type": "color",
                    "value": "#ff5c1a"
                }
            }
        },
        {
            "name": "Button",
            "variables": {
                "kendo-button-text": {
                    "name": "Button text color",
                    "type": "color",
                    "value": "#ffffff"
                },
                "kendo-button-bg": {
                    "name": "Button background",
                    "type": "color",
                    "value": "#5798be"
                }
            }
        },
        {
            "name": "Link",
            "variables": {
                "kendo-link-text": {
                    "name": "Link text color",
                    "type": "color",
                    "value": "#ff5c1a"
