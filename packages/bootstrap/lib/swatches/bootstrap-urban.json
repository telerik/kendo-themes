                "kendo-link-hover-text": {
                    "name": "Link hover text color",
                    "type": "color",
                    "value": "#e65317"
                }
            }
        },
        {
            "name": "Dataviz",
            "variables": {
                "kendo-series-a": {
                    "name": "Series A",
                    "type": "color",
                    "value": "#ff7943"
                },
                "kendo-series-b": {
                    "name": "Series B",
                    "type": "color",
                    "value": "#e84d0e"
                },
                "kendo-series-c": {
                    "name": "Series C",
                    "type": "color",
                    "value": "#40a9e3"
                },
                "kendo-series-d": {
                    "name": "Series D",
                    "type": "color",
                    "value": "#016a98"
                },
                "kendo-series-e": {
                    "name": "Series E",
                    "type": "color",
                    "value": "#7a8c95"
                },
                "kendo-series-f": {
                    "name": "Series F",
                    "type": "color",
                    "value": "#355261"
                }
            }
        },
        {
            "hidden": true,
            "variables": {
                "primary": {
                    "name": "Override bootstrap primary",
                    "type": "variable",
                    "value": "$kendo-color-primary"
                }
            }
        }
    ]
}
