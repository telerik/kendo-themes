{
  "name": "@progress/kendo-theme-bootstrap",
  "description": "Bootstrap v4 theme for Kendo UI",
  "version": "4.15.0",
  "author": "Progress",
  "license": "Apache-2.0",
  "keywords": [
    "Kendo UI",
    "Bootstrap",
    "Theme"
  ],
  "main": "dist/all.css",
  "sass": "scss/all.scss",
  "files": [
    "dist/",
    "scss/",
    "modules/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-themes.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "sass:swatches": "gulp sass:swatches",
    "sass:flat": "gulp sass:flat",
    "dart": "gulp dart",
    "dart:watch": "gulp dart:watch",
    "dart:swatches": "gulp dart:swatches",
    "dart:flat": "gulp dart:flat",
    "lint": "gulp lint",
    "docs": "gulp docs",
    "docs:check": "gulp docs:check",
    "ci": "gulp ci",
    "ci:full": "gulp ci:full",
    "embed-assets": "gulp assets",
    "prepublishOnly": "./build/embed-dependencies 'bootstrap' '@progress/kendo-theme-default' && gulp prepublish",
    "postpublish": "rm -rf modules && git checkout scss"
  },
  "peerDependencies": {
    "bootstrap": "^4.4.1"
  },
  "devDependencies": {
    "@progress/kendo-theme-default": "^4.17.0",
    "@progress/kendo-theme-tasks": "^0.6.1",
    "bootstrap": "^4.4.1",
    "gulp": "^4.0.2"
  }
}
