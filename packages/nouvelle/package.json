{
  "name": "@progress/kendo-theme-nouvelle",
  "description": "A css variables heavy theme for Kendo UI",
  "version": "5.0.0-alpha.2",
  "author": "Progress",
  "license": "Apache-2.0",
  "keywords": [
    "Kendo UI",
    "Theme",
    "Nouvelle",
    "css variables"
  ],
  "main": "dist/index.css",
  "sass": "scss/index.scss",
  "files": [
    "dist/",
    "scss/",
    "lib/"
  ],
  "homepage": "https://github.com/telerik/kendo-themes/tree/master/packages/nouvelle",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-themes.git",
    "directory": "packages/nouvelle"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-themes/issues/new?labels=bug,T:Nouvelle&template=bug_report.md"
  },
  "publishConfig": {
    "access": "public"
  },
  "kendo": {
    "themebuilder": {
      "swatches": {
        "path": "lib/swatches",
        "default": "nouvelle-main.json"
      }
    }
  },
  "scripts": {
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "sass:swatches": "gulp sass:swatches",
    "sass:flat": "gulp sass:flat",
    "dart": "gulp dart",
    "dart:watch": "gulp dart:watch",
    "dart:swatches": "gulp dart:swatches",
    "dart:flat": "gulp dart:flat",
    "prepublishOnly": "echo 'no prebublish for nouvelle theme'",
    "postpublish": "echo 'no postpublish for nouvelle theme'"
  },
  "dependencies": {
    "@progress/kendo-theme-default": "^5.0.0-alpha.2"
  }
}
