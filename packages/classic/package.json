{
  "name": "@progress/kendo-theme-classic",
  "description": "Sass port of less based themes for Kendo UI theme",
  "version": "4.42.1-dev.3",
  "author": "Progress",
  "license": "Apache-2.0",
  "keywords": [
    "Kendo UI",
    "Theme",
    "Classic"
  ],
  "main": "dist/all.css",
  "sass": "scss/all.scss",
  "files": [
    "dist/",
    "scss/",
    "modules/",
    "lib/"
  ],
  "homepage": "https://github.com/telerik/kendo-themes/tree/master/packages/classic",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-themes.git",
    "directory": "packages/classic"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-themes/issues/new?labels=bug,T:Classic&template=bug_report.md"
  },
  "publishConfig": {
    "access": "public"
  },
  "kendo": {
    "themebuilder": {
      "swatches": {
        "path": "lib/swatches",
        "default": "classic-main.json"
      }
    }
  },
  "scripts": {
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "sass:swatches": "gulp sass:swatches",
    "sass:flat": "gulp sass:flat",
    "dart": "gulp dart",
    "dart:watch": "gulp dart:watch",
    "dart:swatches": "gulp dart:swatches",
    "dart:flat": "gulp dart:flat",
    "prepublishOnly": "./build/embed-dependencies '@progress/kendo-theme-default'",
    "postpublish": "rm -rf modules && git checkout scss"
  },
  "dependencies": {
    "@progress/kendo-theme-default": "^4.42.1-dev.3"
  }
}
