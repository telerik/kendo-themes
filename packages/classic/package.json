{
  "name": "@progress/kendo-theme-classic",
  "description": "Sass port of less based themes for Kendo UI theme",
  "version": "5.8.2-dev.0",
  "author": "Progress",
  "license": "Apache-2.0",
  "keywords": [
    "Kendo UI",
    "Theme",
    "Classic"
  ],
  "main": "dist/all.css",
  "sass": "scss/all.scss",
  "files": [
    "dist/",
    "scss/",
    "lib/"
  ],
  "homepage": "https://github.com/telerik/kendo-themes/tree/master/packages/classic",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-themes.git",
    "directory": "packages/classic"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-themes/issues/new?labels=bug,T:Classic&template=bug_report.md"
  },
  "publishConfig": {
    "access": "public"
  },
  "kendo": {
    "themebuilder": {
      "swatches": {
        "path": "lib/swatches",
        "default": "classic-main.json"
      }
    }
  },
  "scripts": {
    "build": "sass-build",
    "sass": "sass-build",
    "prepublishOnly": "node ../../scripts/themes-prepublish.js",
    "postpublish": "echo 'no postpublish for classic theme'"
  },
  "dependencies": {
    "@progress/kendo-theme-default": "^5.8.2-dev.0"
  },
  "devDependencies": {
    "sass-build": "^1.0.0"
  }
}
