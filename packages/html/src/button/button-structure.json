[
    {
        "component": "button",
        "version": "5.3.1",
        "selector": [
            ".k-button"
        ],
        "states": [
            {
                "name": "hover",
                "selector": [
                    ":hover",
                    ".k-hover"
                ]
            },
            {
                "name": "focus",
                "selector": [
                    ":focus",
                    ".k-focus"
                ]
            },
            {
                "name": "active",
                "selector": [
                    ":active",
                    ".k-active"
                ]
            },
            {
                "name": "selected",
                "selector": [
                    ".k-selected"
                ]
            },
            {
                "name": "disabled",
                "selector": [
                    ":disabled",
                    "[disabled]",
                    ".k-disabled"
                ]
            }
        ],
        "options": [
            {
                "name": "size",
                "values": [
                    {
                        "name": "small",
                        "selector": [
                            ".k-button-sm"
                        ]
                    },
                    {
                        "name": "medium",
                        "selector": [
                            ".k-button-md"
                        ]
                    },
                    {
                        "name": "large",
                        "selector": [
                            ".k-button-lg"
                        ]
                    }
                ]
            },
            {
                "name": "rounded",
                "values": [
                    {
                        "name": "small",
                        "selector": [
                            ".k-rounded-sm"
                        ]
                    },
                    {
                        "name": "medium",
                        "selector": [
                            ".k-rounded-md"
                        ]
                    },
                    {
                        "name": "large",
                        "selector": [
                            ".k-rounded-lg"
                        ]
                    },
                    {
                        "name": "full",
                        "selector": [
                            ".k-rounded-full"
                        ]
                    }
                ]
            },
            {
                "name": "fillMode",
                "values": [
                    {
                        "name": "solid",
                        "selector": [
                            ".k-button-solid"
                        ]
                    },
                    {
                        "name": "flat",
                        "selector": [
                            ".k-button-flat"
                        ]
                    },
                    {
                        "name": "outline",
                        "selector": [
                            ".k-button-outline"
                        ]
                    },
                    {
                        "name": "link",
                        "selector": [
                            ".k-button-link"
                        ]
                    }
                ]
            },
            {
                "name": "themeColor",
                "values": [
                    {
                        "name": "base",
                        "selector": [
                            ".k-button-{options.fillMode}-base"
                        ]
                    },
                    {
                        "name": "primary",
                        "selector": [
                            ".k-button-{options.fillMode}-primary"
                        ]
                    },
                    {
                        "name": "secondary",
                        "selector": [
                            ".k-button-{options.fillMode}-secondary"
                        ]
                    },
                    {
                        "name": "tertiary",
                        "selector": [
                            ".k-button-{options.fillMode}-tertiary"
                        ]
                    },
                    {
                        "name": "info",
                        "selector": [
                            ".k-button-{options.fillMode}-info"
                        ]
                    },
                    {
                        "name": "success",
                        "selector": [
                            ".k-button-{options.fillMode}-success"
                        ]
                    },
                    {
                        "name": "warning",
                        "selector": [
                            ".k-button-{options.fillMode}-warning"
                        ]
                    },
                    {
                        "name": "error",
                        "selector": [
                            ".k-button-{options.fillMode}-error"
                        ]
                    },
                    {
                        "name": "dark",
                        "selector": [
                            ".k-button-{options.fillMode}-dark"
                        ]
                    },
                    {
                        "name": "light",
                        "selector": [
                            ".k-button-{options.fillMode}-light"
                        ]
                    },
                    {
                        "name": "inverse",
                        "selector": [
                            ".k-button-{options.fillMode}-inverse"
                        ]
                    }
                ]
            }
        ],
        "children": [
            {
                "name": "icon",
                "ref": "icon",
                "selector": [
                    ".k-button-icon"
                ]
            },
            {
                "name": "text",
                "selector": [
                    ".k-button-text"
                ]
            }
        ]
    }
]
