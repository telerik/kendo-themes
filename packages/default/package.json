{
  "name": "@progress/kendo-theme-default",
  "description": "SASS resources for the default Kendo UI theme",
  "version": "4.43.1-dev.6",
  "author": "Progress",
  "license": "Apache-2.0",
  "keywords": [
    "Kendo UI",
    "Theme"
  ],
  "main": "dist/all.css",
  "sass": "scss/all.scss",
  "files": [
    "dist/",
    "scss/",
    "modules/",
    "lib/"
  ],
  "homepage": "https://github.com/telerik/kendo-themes/tree/master/packages/default",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-themes.git",
    "directory": "packages/default"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-themes/issues/new?labels=bug,T:Default&template=bug_report.md"
  },
  "publishConfig": {
    "access": "public"
  },
  "kendo": {
    "themebuilder": {
      "swatches": {
        "path": "lib/swatches",
        "default": "default-main.json"
      }
    }
  },
  "scripts": {
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "sass:swatches": "gulp sass:swatches",
    "sass:flat": "gulp sass:flat",
    "dart": "gulp dart",
    "dart:watch": "gulp dart:watch",
    "dart:swatches": "gulp dart:swatches",
    "dart:flat": "gulp dart:flat",
    "prepublishOnly": "echo 'no prebublish for default theme'"
  }
}
