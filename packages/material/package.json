{
  "name": "@progress/kendo-theme-material",
  "description": "Material theme for Kendo UI",
  "version": "2.5.1",
  "author": "Progress",
  "license": "Apache-2.0",
  "keywords": [
    "Kendo UI",
    "Material",
    "Material design",
    "Theme"
  ],
  "main": "dist/all.css",
  "sass": "scss/all.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-themes.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "gulp lint",
    "api": "gulp api",
    "api-check": "npm run api && git diff --exit-code --quiet -- docs/ || (echo -e '\\033[0;31mERROR: API docs are out of date' && exit 1)",
    "build": "gulp sass",
    "watch": "gulp sass:watch",
    "embed-assets": "gulp assets",
    "swatches": "gulp swatches",
    "test": "npm run lint && npm run build && npm run api-check",
    "prepublishOnly": "bash ./build/embed-dependencies '@progress/kendo-theme-default' && npm run build && npm run swatches",
    "postpublish": "rm -rf modules && git checkout scss"
  },
  "devDependencies": {
    "@progress/kendo-theme-default": "^3.6.1",
    "@progress/kendo-theme-tasks": "^0.1.5",
    "gulp": "^4.0.2"
  }
}
