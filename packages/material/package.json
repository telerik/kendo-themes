{
  "name": "@progress/kendo-theme-material",
  "description": "Material theme for Kendo UI",
  "version": "3.34.0",
  "author": "Progress",
  "license": "Apache-2.0",
  "keywords": [
    "Kendo UI",
    "Theme",
    "Material",
    "Material design"
  ],
  "main": "dist/all.css",
  "sass": "scss/all.scss",
  "files": [
    "dist/",
    "scss/",
    "modules/",
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-themes.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "kendo": {
    "themebuilder": {
      "swatches": {
        "path": "lib/themebuilder",
        "default": "main.json"
      }
    }
  },
  "scripts": {
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "sass:swatches": "gulp sass:swatches",
    "sass:flat": "gulp sass:flat",
    "dart": "gulp dart",
    "dart:watch": "gulp dart:watch",
    "dart:swatches": "gulp dart:swatches",
    "dart:flat": "gulp dart:flat",
    "prepublishOnly": "./build/embed-dependencies '@progress/kendo-theme-default'",
    "postpublish": "rm -rf modules && git checkout scss"
  },
  "devDependencies": {
    "@progress/kendo-theme-default": "^4.39.0"
  }
}
