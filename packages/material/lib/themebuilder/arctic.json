{
    "$schema": "https://unpkg.com/@progress/kendo-theme-tasks@1.5.0/lib/schemas/themebuilder-swatch.json",
    "name": "Arctic",
    "base": "@progress/kendo-theme-material",
    "hidden": true,
    "previewColors": [
        "#ffffff",
        "#cfd8dc",
        "#2196f3",
        "#cddc39",
        "#455a64"
    ],
    "components": [],
    "themeBuilder": [
        {
            "name": "",
            "variables": {
                "border-radius": {
                    "name": "Border radius",
                    "type": "number",
                    "value": "2px"
                }
            }
        },
        {
            "name": "Theme colors",
            "variables": {
                "primary-palette-name": {
                    "name": "Primary",
                    "type": "colorlist",
                    "enum": [
                        "teal",
                        "cyan",
                        "lightBlue",
                        "blue",
                        "indigo",
                        "green",
                        "blueGrey",
                        "deepPurple",
                        "brown",
                        "lightGreen",
                        "purple",
                        "grey",
                        "lime",
                        "pink",
                        "red",
                        "orange",
                        "amber",
                        "yellow"
                    ],
                    "value": "blue"
                },
                "secondary-palette-name": {
                    "name": "Secondary",
                    "type": "colorlist",
                    "enum": [
                        "teal",
                        "cyan",
                        "lightBlue",
                        "blue",
                        "indigo",
                        "green",
                        "blueGrey",
                        "deepPurple",
                        "brown",
                        "lightGreen",
                        "purple",
                        "grey",
                        "lime",
                        "pink",
                        "red",
                        "orange",
                        "amber",
                        "yellow"
                    ],
                    "value": "lime"
                },
                "theme-type": {
                    "name": "Base",
                    "type": "colorlist",
                    "enum": [
                        "light",
                        "dark"
                    ],
                    "value": "light"
                }
            }
        }
    ]
}
