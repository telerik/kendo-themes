{
    "$schema": "@progress/kendo-theme-tasks/lib/schemas/themebuilder-swatch.json",
    "name": "Material",
    "base": "@progress/kendo-theme-material",
    "previewColors": [ "#ffffff", "#e0e0e0", "#3f51b5", "#e91e63", "#424242" ],
    "components": [ ],
    "themeBuilder": [
        {
            "name": "",
            "variables": {
                "border-radius": {
                    "name": "Border radius",
                    "type": "number",
                    "value": "2px"
