        },
        {
            "name": "Theme colors",
            "variables": {
                "primary-palette-name": {
                    "name": "Primary",
                    "type": "colorlist",
                    "enum": [ "teal", "cyan", "lightBlue", "blue", "indigo", "green", "blueGrey", "deepPurple", "brown", "lightGreen", "purple", "grey", "lime", "pink", "red", "orange", "amber", "yellow" ],
                    "value": "indigo"
                },
                "secondary-palette-name": {
                    "name": "Secondary",
                    "type": "colorlist",
                    "enum": [ "teal", "cyan", "lightBlue", "blue", "indigo", "green", "blueGrey", "deepPurple", "brown", "lightGreen", "purple", "grey", "lime", "pink", "red", "orange", "amber", "yellow" ],
                    "value": "pink"
                },
                "theme-type": {
                    "name": "Base",
                    "type": "colorlist",
                    "enum": [ "light", "dark" ],
                    "value": "light"
                }
            }
        }
    ]
}
